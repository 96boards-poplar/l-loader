# Poplar Linux recovery U-Boot script
# Created Tue Jan 23 15:48:53 GMT 2018

# Partition layout (MBR and EBRs)
fatload usb 0:1 0x07800000 recovery_files/install-layout.scr
source 0x07800000

# Partition 1 (loader)
fatload usb 0:1 0x07800000 recovery_files/install-partition1.scr
source 0x07800000

echo ============ INSTALLATION IS DONE =============
echo (Please reset your board)
